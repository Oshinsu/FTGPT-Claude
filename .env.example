# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Mistral AI Configuration (optional)
MISTRAL_API_KEY=your_mistral_api_key_here

# France Travail API
FRANCE_TRAVAIL_CLIENT_ID=your_client_id_here
FRANCE_TRAVAIL_CLIENT_SECRET=your_client_secret_here
FRANCE_TRAVAIL_API_BASE_URL=https://api.francetravail.io/partenaire

# LangSmith (optional monitoring)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=france-travail-gpt

# App Configuration
APP_ENV=development
APP_DEBUG=true
VECTOR_STORE_TYPE=chromadb
VECTOR_STORE_PATH=./data/vector_store

# Model Configuration
MODEL_PROVIDER=openai  # or mistral
MODEL_NAME=gpt-4o
MODEL_TEMPERATURE=0.7
